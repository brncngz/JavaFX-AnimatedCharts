<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BrandGraphController">
<children>
    <AnchorPane prefHeight="538.0" prefWidth="727.0">
        <children>
            <LineChart fx:id="barChart" layoutX="37.0" layoutY="24.0" prefHeight="460.0" prefWidth="646.0" title="Most Revenue Brands over the years">
                <xAxis>
                    <NumberAxis fx:id="yearAxis" label="Value" />
                </xAxis>
                <yAxis>
                    <CategoryAxis fx:id="citiesAxis" label="Companies" side="LEFT" />
                </yAxis>
            </LineChart>
            <Button layoutX="11.0" layoutY="499.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
        </children>
    </AnchorPane>
</children>
<columnConstraints>
    <ColumnConstraints />
</columnConstraints>
<rowConstraints>
    <RowConstraints />
</rowConstraints>
</GridPane>
